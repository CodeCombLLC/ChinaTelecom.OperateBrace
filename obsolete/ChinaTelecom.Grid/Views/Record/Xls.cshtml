@model IEnumerable<Record>
@{
    Layout = null;
}
<table class="table">
    <tr>
        <td>接入号</td>
        <td>用户状态</td>
        <td>用户姓名</td>
        <td>四级承包人名称</td>
        <td>四级承包体名称</td>
        <td>包服人</td>
        <td>当月出账</td>
        <td>中投</td>
        <td>代理费</td>
        <td>一次佣金</td>
        <td>欠费</td>
        <td>装机地址</td>
        <td>标准地址</td>
        <td>套餐</td>
        <td>身份证号码</td>
        <td>联系电话</td>
        <td>是否家庭融合宽带</td>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.Account</td>
            <td>
                @if (x.Status == ServiceStatus.单向欠停)
                {
                    @("欠停(单向)")
                }
                else if (x.Status == ServiceStatus.双向欠停)
                {
                    @("欠停(双向)")
                }
                else
                { 
                    @x.Status
                }
            </td>
            <td>@x.CustomerName</td>
            <td>@x.ContractorName</td>
            <td>@x.ContractorStruct</td>
            <td>@x.ServiceStaff</td>
            <td>@x.CurrentMonthBill</td>
            <td>@x.MDS</td>
            <td>@x.AgentFee</td>
            <td>@x.Commission</td>
            <td>@x.Arrearage</td>
            <td>@x.ImplementAddress</td>
            <td>@x.StandardAddress</td>
            <td>@x.Set</td>
            <td>@x.PRCID</td>
            <td>@x.Phone</td>
            <td>@(x.IsFuse ? "是" : "否")</td>
        </tr>
    }
</table>